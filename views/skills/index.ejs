<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>My Dev Skills</title>
</head>
<body>
    <a href="/">Home</a>
    <h1>My Dev Skills</h1>
    <h3>Skills as of <%= date %>:</h3>
    <ul>
        <% skills.forEach(function(skill) { %>
          <li>
            <a href="/skills/<%= skill.id %>"><%= skill.skill %></a>
          </li>
        <% }); %>
    </ul>
    <a href="/skills/new">Add a new skill</a>
</body>
</html> 